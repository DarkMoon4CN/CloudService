<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Bsr.Cloud.Model" namespace="Bsr.Cloud.Model.Entities">
  <class name="Device" table="Device" lazy="true">
    <id name="DeviceId" column="DeviceId">
      <!-- 主键自增用 native   普通主键 用 assigend -->
      <!--<generator class="assigned"></generator>-->
      <generator class="native"></generator>
    </id>
    <property name="DeviceName" column="DeviceName"></property>
    <property name="DeviceType" column="DeviceType"></property>
    <property name="CustomerId" column="CustomerId"></property>
    <property name="SerialNumber" column="SerialNumber"></property>
    <property name="BPServerDeviceId" column="BPServerDeviceId"></property>
    <property name="BPServerConfigId" column="BPServerConfigId"></property>
    <property name="HardwareType" column="HardwareType"></property>
    <property name="UserName" column="UserName"></property>
    <property name="PassWord" column="PassWord"></property>
  </class>
</hibernate-mapping>